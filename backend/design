#! /opt/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1_1)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x7fc090600100 .scope module, "first_counter_tb" "first_counter_tb" 2 50;
 .timescale 0 0;
v0x7fc090617bf0_0 .var "clock", 0 0;
v0x7fc090617c90_0 .net "counter_out", 3 0, v0x7fc090617990_0;  1 drivers
v0x7fc090617d40_0 .var "enable", 0 0;
v0x7fc090617e10_0 .var "reset", 0 0;
S_0x7fc090607830 .scope module, "U_counter" "first_counter" 2 76, 2 9 0, S_0x7fc090600100;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 1 "enable"
    .port_info 3 /OUTPUT 4 "counter_out"
v0x7fc090607af0_0 .net "clock", 0 0, v0x7fc090617bf0_0;  1 drivers
v0x7fc090617990_0 .var "counter_out", 3 0;
v0x7fc090617a40_0 .net "enable", 0 0, v0x7fc090617d40_0;  1 drivers
v0x7fc090617af0_0 .net "reset", 0 0, v0x7fc090617e10_0;  1 drivers
E_0x7fc090601ec0 .event posedge, v0x7fc090607af0_0;
S_0x7fc090607990 .scope begin, "COUNTER" "COUNTER" 2 35, 2 35 0, S_0x7fc090607830;
 .timescale 0 0;
    .scope S_0x7fc090607830;
T_0 ;
    %wait E_0x7fc090601ec0;
    %fork t_1, S_0x7fc090607990;
    %jmp t_0;
    .scope S_0x7fc090607990;
t_1 ;
    %load/vec4 v0x7fc090617af0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x7fc090617990_0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fc090617a40_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x7fc090617990_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x7fc090617990_0, 1;
T_0.2 ;
T_0.1 ;
    %end;
    .scope S_0x7fc090607830;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fc090600100;
T_1 ;
    %vpi_call 2 57 "$display", "time\011 clk reset enable counter" {0 0 0};
    %vpi_call 2 58 "$monitor", "%g\011 %b   %b     %b      %b", $time, v0x7fc090617bf0_0, v0x7fc090617e10_0, v0x7fc090617d40_0, v0x7fc090617c90_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fc090617bf0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fc090617e10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fc090617d40_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fc090617e10_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fc090617e10_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fc090617d40_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fc090617d40_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 67 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x7fc090600100;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x7fc090617bf0_0;
    %inv;
    %store/vec4 v0x7fc090617bf0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
